/*
Write a program that prints all the numbers from 1 to N.
*/
using System;

class PrintNumbersOnetoN
{
    static void Main()
    {
        Console.Write("Vyvedete chislo, do koeto iskate da printirate vsichki chisla, zapochvaiki ot 1:");
        int n = int.Parse(Console.ReadLine());
        int number = 1;
        while (number <= n)
        {
            Console.WriteLine(number);
            number++;
        }
    }
}
